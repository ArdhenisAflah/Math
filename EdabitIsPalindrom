using System;

//Level:Medium 
public class Program
{
    public static bool IsPalindrome(int num)
    {
			 string res = num.ToString();
			
			//ubah ke char array
			char[] res1 = res.ToCharArray();
			//reverse
			 Array.Reverse(res1);
			 string sw = new string(res1);
			
			 if(res == sw){
				 return true;
				 
			 }else{
				 return false;
			 
			}
    }
}
